{
  "name": "agentic-sage-coordinator",
  "version": "0.1.0",
  "description": "Monorepo root for agentic-sage-coordinator packages.",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "eslint packages/*/src",
    "format": "prettier --write \"packages/*/src/**/*.ts\"",
    "format:check": "prettier --check \"packages/*/src/**/*.ts\""
  },
  "keywords": [
    "saga",
    "ai",
    "agent",
    "orchestration",
    "typescript",
    "compensation",
    "distributed-systems"
  ],
  "author": "franruedaesq",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/franruedaesq/agentic-sage-coordinator.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.0.0"
  }
}
